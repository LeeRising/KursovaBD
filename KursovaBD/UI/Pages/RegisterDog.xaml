<UserControl x:Class="KursovaBD.UI.Pages.RegisterDog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:KursovaBD.UI.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="520" d:DesignWidth="520">
    <StackPanel>
        <StackPanel Orientation="Horizontal" Margin="10">
            <TextBlock Text="Dog name:"/>
            <TextBox x:Name="DogNameTb" Width="150"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal" Margin="10">
            <TextBlock Text="Dog name:"/>
            <TextBox x:Name="DogNameTb1" Width="150"/>
        </StackPanel>
    </StackPanel>
</UserControl>
